{
  "api": {
    "inventory": {
      "dependsOn": [],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [],
          "defaultAuthentication": {
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7
            },
            "authenticationType": "API_KEY"
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "auth": {
    "inventorypoc": {
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL"
        ],
        "socialProviders": [],
        "usernameAttributes": [],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito",
      "serviceType": "managed"
    }
  },
  "function": {
    "ProductLambda": {
      "build": true,
      "dependsOn": [
        {
          "attributeEnvMap": {
            "Arn": "TABLE_ARN",
            "Name": "TABLE_NAME"
          },
          "attributes": [
            "Name",
            "Arn"
          ],
          "category": "storage",
          "resourceName": "products"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "hosting": {
    "S3AndCloudFront": {
      "providerPlugin": "awscloudformation",
      "service": "S3AndCloudFront"
    }
  },
  "parameters": {
    "AMPLIFY_function_ProductLambda_deploymentBucketName": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "ProductLambda"
        }
      ]
    },
    "AMPLIFY_function_ProductLambda_s3Key": {
      "usedBy": [
        {
          "category": "function",
          "resourceName": "ProductLambda"
        }
      ]
    }
  },
  "storage": {
    "products": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}